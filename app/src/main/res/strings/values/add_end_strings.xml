    <string name="help_input_end__end_inputs_arrows_title">End</string>
    <string name="help_input_end__end_inputs_arrows_body">Individual arrow values for the current end. Note: \'X\' on a standard target face is the inner most ring</string>
    <string name="help_input_end__end_inputs_clear_title">Clear</string>
    <string name="help_input_end__end_inputs_clear_body">Removes all arrows from the current end</string>
    <string name="help_input_end__end_inputs_backspace_title">Backspace</string>
    <string name="help_input_end__end_inputs_backspace_body">Deletes the last arrow inputted on the current end</string>
